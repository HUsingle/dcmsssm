<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org//dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcms.dao.ApplyMapper">
    <!--个人报名-->
    <insert id="insertOneSelfInfo">
        insert into db_apply values(0,#{param1},#{param2},now(),0,0 ,'',0,#{param3})
    </insert>
    <!--查询个人赛是否已经报名-->
    <select id="isExistSelfInfo" resultType="java.lang.String">
        select c_group_name from db_apply where s_number = #{param1} and c_id = #{param2}
    </select>
</mapper>